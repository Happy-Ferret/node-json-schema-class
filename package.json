{
  "name": "json-schema-class",
  "version": "0.0.1",
  "description": "An Abstract Class that uses JSON Schema validation to build an extensible application with strongish type",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/babel-cli/bin/babel-node.js ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha  -- ./src/SchemaClass-test.js -R spec",
    "prepublish": "node ./node_modules/babel-cli/bin/babel.js -d lib src/ --ignore src/*-*.js*",
    "docs": "node ./bin/acquit/generator && esdoc -c esdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeJunkie/node-json-schema-class.git"
