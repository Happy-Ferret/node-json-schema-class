  },
  "homepage": "https://github.com/NodeJunkie/node-json-schema-class#readme",
  "devDependencies": {
    "acquit": "^0.4.1",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "esdoc": "^0.4.6",
    "esdoc-es7-plugin": "0.0.3",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
